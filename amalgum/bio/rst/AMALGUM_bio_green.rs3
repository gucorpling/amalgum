<segment id="116" parent="1116" relname="span">Allegory</segment>
<segment id="117" parent="116" relname="elaboration">( 1977 )</segment>
<segment id="118" parent="1118" relname="preparation">Gary Green , Vol. 3 :</segment>
<segment id="119" parent="1118" relname="span">Still at Large</segment>
<segment id="120" parent="119" relname="elaboration">( 1982 )</segment>
