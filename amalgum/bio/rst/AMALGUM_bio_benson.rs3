<segment id="88" parent="1083" relname="joint">Virgin and Child with Saints , date unknown</segment>
<segment id="89" parent="1083" relname="joint">Deipara Virgo , date unknown</segment>
<segment id="90" parent="1083" relname="joint">Elegant couples dancing in a landscape ,</segment>
<segment id="91" parent="1083" relname="joint">date unknown .</segment>
<segment id="92" parent="1093" relname="span">Currently exhibited at the Utah Museum of Fine Arts</segment>
<segment id="93" parent="92" relname="restatement">( UMFA ) .</segment>
