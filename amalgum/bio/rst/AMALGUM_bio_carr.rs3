<segment id="121" parent="120" relname="elaboration">Nominated for National Lottery Good Causes Award</segment>
<segment id="122" parent="1121" relname="evidence">[ 10 ]</segment>
<segment id="123" parent="1122" relname="joint">2013 : Polish Daily Award for Culture</segment>
<segment id="124" parent="1124" relname="span">2013 :</segment>
<segment id="125" parent="124" relname="elaboration">Nominated for Best Artist , Songlines Music Awards</segment>
<segment id="126" parent="1126" relname="span">2014 : Best Concept Album , Paszport , Independent Music Awards</segment>
<segment id="127" parent="126" relname="evidence">[ 15 ]</segment>
<segment id="128" parent="129" relname="preparation">2014 :</segment>
<segment id="129" parent="1128" relname="span">Honorary membership , Polish 1st Armoured Division</segment>
<segment id="130" parent="1130" relname="span">2016 : Pro Patria Medal</segment>
<segment id="131" parent="130" relname="elaboration">( Poland )</segment>
