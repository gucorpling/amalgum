<segment id="1" parent="1001" relname="span">Boki</segment>
<segment id="2" parent="1" relname="elaboration">( Hawaiian chief )</segment>
<segment id="3" parent="1004" relname="span">For other people or places</segment>
<segment id="4" parent="3" relname="elaboration">named " Boki " , see Boki</segment>
<segment id="5" parent="1004" relname="elaboration">( disambiguation ) .</segment>
<segment id="6" parent="1008" relname="span">Warning :</segment>
<segment id="7" parent="1010" relname="span">Page using Template :</segment>
