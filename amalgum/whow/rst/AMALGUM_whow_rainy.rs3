<segment id="170" parent="169" relname="elaboration">You 'll Need</segment>
<segment id="171" parent="1171" relname="preparation">blankets</segment>
<segment id="172" parent="1171" relname="joint">bedsheet , size queen or larger</segment>
<segment id="173" parent="1173" relname="preparation">two chairs</segment>
