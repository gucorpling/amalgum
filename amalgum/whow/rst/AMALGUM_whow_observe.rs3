<segment id="43" parent="1041" relname="sequence">Draco</segment>
<segment id="44" parent="1041" relname="sequence">Cygnus</segment>
<segment id="45" parent="1041" relname="sequence">Bootes</segment>
<segment id="46" parent="1041" relname="sequence">Canes Venatici</segment>
<segment id="47" parent="1041" relname="sequence">Camelopardalis</segment>
<segment id="48" parent="1041" relname="sequence">Lynx</segment>
<segment id="49" parent="1041" relname="sequence">Cepheus</segment>
