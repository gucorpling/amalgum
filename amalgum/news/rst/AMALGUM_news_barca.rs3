<body>
<segment id="1" parent="1001" relname="preparation">Barca eliminated in Champions League</segment>
<segment id="2" parent="1002" relname="circumstance">Tuesday , March 6 , 2007</segment>
<segment id="3" parent="1003" relname="joint">Liverpool FC 0(4 )</segment>
<segment id="4" parent="1003" relname="joint">1(3 )</segment>
<segment id="5" parent="1004" relname="joint">FC Barcelona</segment>
<segment id="6" parent="1007" relname="span">Match Stats</segment>
<segment id="7" parent="6" relname="elaboration">Attendance 45000</segment>
<segment id="8" parent="1004" relname="joint">Goalscorers for Liverpool FC</segment>
<segment id="9" parent="1004" relname="joint">None Goalscorers for FC Barcelona</segment>
<segment id="10" parent="1010" relname="span">Gudjohnsen</segment>
<segment id="11" parent="10" relname="restatement">( 75 ' )</segment>
<segment id="12" parent="1012" relname="span">Bookings</segment>
<segment id="13" parent="12" relname="restatement">( Liverpool FC )</segment>
<segment id="14" parent="1014" relname="span">Arbeloa , Sissoko , Pennant , Reina</segment>
<segment id="15" parent="14" relname="elaboration">( Yellow ( 4 ) )</segment>
