<segment id="70" parent="69" relname="elaboration">( 1989 ) .</segment>
<segment id="71" parent="1065" relname="joint">The Andy Warhol Diaries .</segment>
<segment id="72" parent="1072" relname="span">New York :</segment>
<segment id="73" parent="72" relname="elaboration">Warner Books .</segment>
