<segment id="1" parent="2" relname="preparation">Football :</segment>
<segment id="2" parent="1001" relname="span">Everton defeat Fulham 4-1 at Goodison Park</segment>
<segment id="3" parent="1003" relname="circumstance">Friday , April 6 , 2007</segment>
<segment id="4" parent="1005" relname="attribution">Everton FC 4 1 Fulham FC</segment>
<segment id="5" parent="1005" relname="joint">Match Stats</segment>
<segment id="6" parent="1005" relname="joint">Attendance 35612</segment>
<segment id="7" parent="1005" relname="joint">Goalscorers for Everton FC</segment>
<segment id="8" parent="1009" relname="span">Carsley</segment>
<segment id="9" parent="1010" relname="span">( 25 ' ) , Stubbs</segment>
<segment id="10" parent="9" relname="restatement">( 34 ' ) ,</segment>
<segment id="11" parent="1012" relname="span">Vaughan</segment>
