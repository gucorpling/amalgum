<segment id="36" parent="1036" relname="preparation"># D ,</segment>
<segment id="37" parent="1036" relname="joint">Station Grill , 170 W Grand , ☏ +1 805-489-3030 .</segment>
<segment id="38" parent="1036" relname="joint">6:30AM - 8PM .</segment>
<segment id="39" parent="1036" relname="joint">Breakfast , Lunch , and Dinner .</segment>
<segment id="40" parent="1036" relname="joint">Sandwiches , burgers , salads , pasta .</segment>
<segment id="41" parent="1041" relname="span">Casual dining , great for the family .</segment>
<segment id="42" parent="41" relname="elaboration">all food $ 5 - $ 10 .</segment>
<segment id="43" parent="1036" relname="joint">Sleep</segment>
<segment id="44" parent="1036" relname="joint">Holiday Inn Express , 775 N. Oak Park Blvd. , ☏ +1805 481-4448 .</segment>
