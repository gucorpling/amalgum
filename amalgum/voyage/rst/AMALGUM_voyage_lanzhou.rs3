<segment id="171" parent="170" relname="restatement">( 皋兰山 )</segment>
<segment id="172" parent="1172" relname="span">Cool place</segment>
<segment id="173" parent="1173" relname="span">to be</segment>
<segment id="174" parent="173" relname="circumstance">when the summer heat kicks in .</segment>
<segment id="175" parent="1175" relname="span">Qiaomen Mosque .</segment>
<segment id="176" parent="175" relname="restatement">( 桥门清真寺 )</segment>
<segment id="177" parent="1177" relname="span">Tianfusha Palace .</segment>
<segment id="178" parent="177" relname="restatement">( 天斧沙宫 )</segment>
<segment id="179" parent="1179" relname="span">Bingling Si .</segment>
<segment id="180" parent="179" relname="restatement">( 炳灵寺石窟 )</segment>
<segment id="181" parent="1134" relname="joint">Cave grottoes and a boat ride , all in one trip .</segment>
