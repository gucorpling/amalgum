<segment id="112" parent="1114" relname="joint">seniors $ 6.80 ,</segment>
<segment id="113" parent="1115" relname="span">families $ 15.70 ,</segment>
<segment id="114" parent="113" relname="elaboration">children under 18 free .</segment>
