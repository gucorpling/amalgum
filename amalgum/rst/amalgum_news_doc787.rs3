<segment id="24" parent="1024" relname="span">Deputy Roberto Jefferson</segment>
<segment id="25" parent="24" relname="elaboration">( file ) .</segment>
<segment id="26" parent="1027" relname="preparation">Photo :</segment>
<segment id="27" parent="1027" relname="span">José Cruz - ABR</segment>
