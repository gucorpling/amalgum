<segment id="145" parent="1145" relname="preparation">For the face mask :</segment>
<segment id="146" parent="1146" relname="preparation">1 tbsp sugar</segment>
<segment id="147" parent="148" relname="preparation">Aloe vera leaf</segment>
<segment id="148" parent="1146" relname="span">1/2 tbsp milk</segment>
