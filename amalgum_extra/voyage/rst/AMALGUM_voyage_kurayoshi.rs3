<segment id="79" parent="1077" relname="joint">Daisen</segment>
<segment id="80" parent="1077" relname="joint">Kotoura</segment>
<segment id="81" parent="1077" relname="joint">Yonago</segment>
<segment id="82" parent="1077" relname="joint">Sakai</segment>
