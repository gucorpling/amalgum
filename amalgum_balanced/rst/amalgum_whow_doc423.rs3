<segment id="106" parent="1106" relname="preparation">Hats</segment>
<segment id="107" parent="1107" relname="preparation">Bandanas</segment>
<segment id="108" parent="1108" relname="span">Pajamas</segment>
<segment id="109" parent="1109" relname="span">( it could get cold at night ,</segment>
<segment id="110" parent="109" relname="purpose">so bring long sleep wear , as well as light weight ones )</segment>
<segment id="111" parent="1111" relname="preparation">Jeans</segment>
<segment id="112" parent="1112" relname="span">Sweatpants or yoga pants</segment>
<segment id="113" parent="112" relname="elaboration">( light weight , and or heavy )</segment>
<segment id="114" parent="1114" relname="preparation">Swim suit</segment>
<segment id="115" parent="1115" relname="span">Jackets</segment>
<segment id="116" parent="115" relname="elaboration">( light weight , and or heavy )</segment>
<segment id="117" parent="1117" relname="span">Sweat shirts</segment>
<segment id="118" parent="117" relname="elaboration">( light weight , and or heavy )</segment>
