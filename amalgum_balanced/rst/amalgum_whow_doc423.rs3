<segment id="95" parent="1095" relname="preparation">Denim</segment>
<segment id="96" parent="1096" relname="preparation">Corduroy</segment>
<segment id="97" parent="1097" relname="preparation">Mesh .</segment>
<segment id="98" parent="99" relname="concession">This is n’t a natural fabric ,</segment>
<segment id="99" parent="1098" relname="span">but it is good to wear in hot weather .</segment>
<segment id="100" parent="1100" relname="preparation">Shorts</segment>
<segment id="101" parent="1101" relname="preparation">Sleeveless shirts</segment>
<segment id="102" parent="1102" relname="preparation">Flip flops</segment>
<segment id="103" parent="1103" relname="preparation">Sneakers , or Crocs</segment>
