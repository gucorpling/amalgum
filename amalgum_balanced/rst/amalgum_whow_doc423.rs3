<segment id="124" parent="123" relname="elaboration">( small towel )</segment>
<segment id="125" parent="1125" relname="preparation">Face soap</segment>
<segment id="126" parent="1126" relname="preparation">Hand soap</segment>
<segment id="127" parent="1127" relname="preparation">Razors</segment>
<segment id="128" parent="1128" relname="preparation">Comb , and or brush</segment>
<segment id="129" parent="1129" relname="preparation">Shampoo , and conditioner</segment>
