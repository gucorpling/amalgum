<segment id="1" parent="1001" relname="preparation">Melbourne Beach</segment>
<segment id="2" parent="1003" relname="joint">Melbourne Beach is a town in Brevard</segment>
<segment id="3" parent="1003" relname="joint">County , Florida .</segment>
<segment id="4" parent="1004" relname="joint">Get in</segment>
<segment id="5" parent="1006" relname="span">Melbourne Intl</segment>
<segment id="6" parent="5" relname="elaboration">( MLB )</segment>
<segment id="7" parent="1008" relname="span">Orlando Intl</segment>
<segment id="8" parent="7" relname="elaboration">( MCO )</segment>
