<segment id="1" parent="1001" relname="span">Boki</segment>
<segment id="2" parent="1" relname="elaboration">( Hawaiian chief )</segment>
<segment id="3" parent="1005" relname="span">For other people or places</segment>
<segment id="4" parent="3" relname="elaboration">named &quot; Boki &quot; ,</segment>
<segment id="5" parent="1006" relname="span">see Boki</segment>
<segment id="6" parent="5" relname="elaboration">( disambiguation ) .</segment>
<segment id="7" parent="1008" relname="preparation">Warning :</segment>
